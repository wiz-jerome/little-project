<view class="logisticsmap">
  <map
    id="map"
    longitude="121.429791"
    latitude="31.297794"
    scale="12"
    controls="{{controls}}"
    bindcontroltap="controltap"
    markers="{{markers}}"
    bindmarkertap="markertap"
    polyline="{{polyline}}"
    bindregionchange="regionchange"
    show-location
    style="width: 100%; height: 100vh;"
  ></map>
  
</view>